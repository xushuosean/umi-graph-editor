{
  "extends": "./src/.umi/tsconfig.json",
  "paths": {
    "@/*": ["src/*"],
    "@@/*": ["./src/.umi/*"] // 解决 umi 找不到部分模块的问题
  },
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@typed-mxgraph"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
